{
    "minimum-stability": "dev",
    "require": {
        "codeception/codeception": "dev-pageobjects",
        "dropbox/dropbox-sdk":     "dev-master",
        "emberlabs/gravatarlib":   "dev-master",
        "fzaninotto/faker":        "1.2.*@dev",
        "php":                     ">=5.3.0",
        "swiftmailer/swiftmailer": "v5.0.1",
        "yiisoft/yii2":            "dev-master",
        "yiisoft/yii2-composer":   "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/aivus/gravatarlib"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\InstallHandler::setPermissions"
        ]
    },
    "extra": {
        "yii-install-writable": [
            "runtime",
            "www/assets"
        ],
        "yii-install-executable": [
            "yii"
        ]
    }
}
